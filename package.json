{
  "name": "svn-wc-attendant",
  "version": "0.0.1",
  "description": "Deals with your dirty Subversion WC so you don't have to.",
  "repository": "https://github.com/prometheas/svn-wc-attendant.git",
  "main": "lib/main.js",
  "scripts": {
    "dc": "docker-compose -f test/services/docker-compose.yml",
    "develop": "docker-compose -f test/services/docker-compose.yml up develop",
    "rebuild-develop": "docker build -t services_develop test/services/node-host",
    "rebuild-svn": "docker build -t services_svn test/services/svn-host",
    "rebuild-test": "docker build -t services_test test/services/node-host",
    "test": "docker-compose -f test/services/docker-compose.yml up test"
  },
  "author": "John Lianoglou",
  "license": "MIT",
  "dependencies": {
    "deasync": "^0.1.4",
    "lodash": "^4.6.1",
    "svn-interface": "^0.5.0",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dir-compare": "^1.0.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^2.2.0",
    "mocha": "^2.4.5",
    "shelljs": "^0.6.0",
    "temp": "^0.8.3"
  },
  "eslintConfig": {
    "extends": "airbnb/legacy",
    "env": {
      "node": true
    }
  }
}
